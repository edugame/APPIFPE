<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Contacts">
        <param name="android-package" value="org.apache.cordova.contacts.ContactManager" />
    </feature>
    <feature name="PushbotsPlugin">
        <param name="android-package" value="com.pushbots.plugin.PushbotsPlugin" />
    </feature>
    <allow-intent href="market:*" />
    <name>Informativo IFPE</name>
    <description>
       app do IFPE .
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Docentes Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" height="36" platform="android" src="www/image/ifpe.png" width="36" />
    <icon density="mdpi" height="48" platform="android" src="www/image/ifpe (1).png" width="48" />
    <icon density="hdpi" height="72" platform="android" src="www/image/ifpe (2).png" width="72" />
    <icon density="xhdpi" height="96" platform="android" src="www/image/ifpe (3).png" width="96" />
    <splash density="ldpi" height="426" orientation="portrait" platform="android" src="www/image/ifpe (4).png" width="320" />
    <splash density="mdpi" height="470" orientation="portrait" platform="android" src="www/image/ifpe (5).png" width="320" />
    <splash density="hdpi" height="640" orientation="portrait" platform="android" src="www/image/ifpe (6).png" width="480" />
    <splash density="xhdpi" height="960" orientation="portrait" platform="android" src="www/image/ifpe (7).png" width="720" />
    <splash density="ldpi" height="320" orientation="landscape" platform="android" src="www/image/ifpe (8).png" width="426" />
    <splash density="mdpi" height="320" orientation="landscape" platform="android" src="www/image/ifpe (9).png" width="470" />
    <splash density="hdpi" height="480" orientation="landscape" platform="android" src="www/image/ifpe (10).png" width="640" />
    <splash density="xhdpi" height="720" orientation="landscape" platform="android" src="www/image/ifpe (11).png" width="960" />
</widget>
