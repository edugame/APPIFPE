<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Informativo IFPE</name>
    <description>
       app do IFPE .
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Docentes Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon platform="android" src="www/image/ifpe.png" density="ldpi" width="36" height="36"/>
    <icon platform="android" src="www/image/ifpe (1).png" density="mdpi" width="48" height="48"/>
    <icon platform="android" src="www/image/ifpe (2).png" density="hdpi" width="72" height="72"/>
    <icon platform="android" src="www/image/ifpe (3).png" density="xhdpi" width="96" height="96"/>
    <splash platform="android" src="www/image/ifpe (4).png" density="ldpi" width="320" height="426" orientation="portrait"/>
    <splash platform="android" src="www/image/ifpe (5).png" density="mdpi" width="320" height="470" orientation="portrait"/>
    <splash platform="android" src="www/image/ifpe (6).png" density="hdpi" width="480" height="640" orientation="portrait"/>
    <splash platform="android" src="www/image/ifpe (7).png" density="xhdpi" width="720" height="960" orientation="portrait"/>
    <splash platform="android" src="www/image/ifpe (8).png" density="ldpi" width="426" height="320" orientation="landscape"/>
    <splash platform="android" src="www/image/ifpe (9).png" density="mdpi" width="470" height="320" orientation="landscape"/>
    <splash platform="android" src="www/image/ifpe (10).png" density="hdpi" width="640" height="480" orientation="landscape"/>
    <splash platform="android" src="www/image/ifpe (11).png" density="xhdpi" width="960" height="720" orientation="landscape"/>
    <platform name="android">
        <allow-intent href="market:*" />
       
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
